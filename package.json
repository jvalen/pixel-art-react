    "buffer": "^6.0.3",
    "canvas-toBlob": "^1.0.0",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.2",
    "gif-encoder": "^0.7.2",
    "gm": "^1.23.1",
    "immutable": "^3.8.2",
    "lost": "^8.3.1",
    "oauth": "^0.9.15",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-reporter": "^6.0.1",
    "precss": "^2.0.0",
    "prop-types": "^15.7.2",
    "pug": "^3.0.3",
    "react": "^16.8.6",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-beautiful-dnd": "^12.1.1",
    "react-color": "^2.18.1",
    "react-cookie-consent": "^5.2.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.9.1",
