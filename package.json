    "react-router-dom": "^6.16.0",
    "react-transform-hmr": "^1.0.4",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.4",
    "redux-undo": "^1.0.0-beta9",
    "shortid": "^2.2.14",
    "stream-browserify": "^3.0.0",
    "style-loader": "^0.23.1",
    "styled-components": "^5.2.0",
    "styled-theming": "^2.2.0",
    "temp": "^0.9.0",
    "tinykeys": "^1.1.0",
    "twitter": "^1.7.1",
    "url-loader": "^4.1.1",
    "util": "^0.12.5",
    "whatwg-fetch": "^2.0.4"
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  },
  "devDependencies": {
